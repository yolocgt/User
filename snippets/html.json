{
    /*
	// Place your snippets for HTML here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
    "HTML文档基本结构": {
        "prefix": "go",
        "body": [
            "<html>",
            "\t<head>",
            "\t\t<meta charset=\"utf-8\">",
            "\t\t<meta name=\"author\" content=\"CGT\">",
            "\t\t<title>$1</title>",
            "\t\t<style>",
            "\t\t\t",
            "\t\t</style>",
            "\t</head>",
            "\t<body>",
            "\t\t<script>",
            "\t\t\t$2",
            "\t\t</script>",
            "\t</body>",
            "<html>"
        ],
        "description": "HTML文档基本结构"
    },
    "HTML+jQuery文档基本结构": {
        "prefix": "jg",
        "body": [
            "<html>",
            "\t<head>",
            "\t\t<meta charset=\"utf-8\">",
            "\t\t<meta name=\"author\" content=\"CGT\">",
            "\t\t<title>$1</title>",
            "\t\t<script src='https://cdn.bootcss.com/jquery/1.12.4/jquery.min.js'></script>",
            "\t\t<link rel='stylesheet' href='https://cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css'>",
            "\t\t<style>",
            "\t\t\t$2",
            "\t\t</style>",
            "\t</head>",
            "\t<body>",
            "\t\t<script>",
            "\t\t\t$3",
            "\t\t</script>",
            "\t</body>",
            "<html>"
        ],
        "description": "HTML+jQuery文档基本结构"
    },
    "al": {
        "prefix": "al",
        "body": [
            "alert(${1:'tt'})"
        ],
        "description": "Anonymous function"
    },
    "Bootstrap文档基本结构": {
        "prefix": "bt",
        "body": [
            "<!DOCTYPE html>",
            "<html lang=\"zh-CN\">",
            "\t<head>",
            "\t\t<meta charset=\"utf-8\">",
            "\t\t<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">",
            "\t\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no\">",
            "\t\t<meta name=\"author\" content=\"CGT\">",
            "\t\t<title>$1</title>",
            "\t\t<link href=\"../bootstrap/css/bootstrap.css\" rel=\"stylesheet\">",
            "\t\t<link href=\"style.css\" rel=\"stylesheet\">",
            "\t</head>",
            "\t<body>",
            "\t\t$2",
            "\t\t<script src=\"./bootstrap/js/jquery.js\"></script>",
            "\t\t<script src=\"./bootstrap/js/bootstrap.js\"></script>",
            "\t</body>",
            "</html>"
        ],
        "description": "Bootstrap文档基本结构"
    },
    "color": {
        "prefix": "wt",
        "body": "width : ${1}px\n height : ${1}px"
    }
}